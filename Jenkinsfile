            steps {
                sh 'echo "Yo Guys"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Im Testing"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Im Deploing"'
            }
        }
    }
}
